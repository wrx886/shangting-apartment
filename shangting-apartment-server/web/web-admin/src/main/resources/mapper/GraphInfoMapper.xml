<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.wrx886.shangting_apartment_server.web.admin.mapper.GraphInfoMapper">
    <select 
        id="selectListByItemTypeAndId"
        resultType="com.github.wrx886.shangting_apartment_server.web.admin.vo.graph.GraphVo"
    >
        SELECT 
            name, 
            url
        FROM graph_info
        WHERE
            is_deleted = 0
            AND item_type = #{itemType} 
            AND item_id = #{id}
    </select>
</mapper>